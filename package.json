{
  "name": "multiplayer-test",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node src/server/server.ts",
    "build": "rimraf dist & webpack --config webpack/webpack.config.js",
    "build:watch": "npm run build -- --watch",
    "test": "mocha --require ts-node/register \"src/**/*.test.ts\"",
    "test:watch": "npm run test -- --watch",
    "prettify": "prettier --write \"src/**/*\" \"webpack/**/*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glinkis/multiplayer-test.git"
  },
  "author": "Glinkis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/multiplayer-test/issues"
  },
  "homepage": "https://github.com/Glinkis/multiplayer-test#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "mocha": "^5.0.1",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/express": "^4.11.1",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/socket.io": "^1.4.31",
    "@types/webpack": "^3.8.8",
    "awesome-typescript-loader": "^3.4.1",
    "html-webpack-plugin": "^2.30.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "typescript": "^2.7.2",
    "typestyle": "^1.7.1",
    "webpack": "^3.11.0",
    "webpack-split-by-path": "^2.0.0"
  }
}
